# -2 3 -1 2 -6 3
a = [-2, 3, -1, 2, -6, 3]

b = [0] * len(a)
# b[k]: Tổng lớn nhất của dãy con k phần tử kết thúc tại vị trí k

b[0] = a[0]

for k in range(1, len(a)):
    if b[k - 1] > 0: # b[k - 1] + a[k] > a[k]
        b[k] = b[k - 1] + a[k]
    else:
        b[k] = a[k]

tong = 0
end = 0
for k in range(len(b)):
    if b[k] > tong:
        tong = b[k]
        end = k
 
kq = []

while tong != 0:
    kq.append(a[end])
    tong -= a[end]
    end -= 1

print(kq[::-1])